<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/parent_pricing"
    tools:context=".fragments.pricing.Pricing">


    <TextView
        android:id="@+id/planstv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Plans"
        android:textColor="@color/colorWhite"
        android:textSize="@dimen/textsize_24sp"
        android:gravity="center"
        android:layout_marginTop="@dimen/default_margin_16dp">

    </TextView>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/priciing_recyclerview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="visible"
        android:layout_marginTop="55dp">


    </androidx.recyclerview.widget.RecyclerView>


<!--    <include layout="@layout/price_layout"></include>-->

</RelativeLayout>